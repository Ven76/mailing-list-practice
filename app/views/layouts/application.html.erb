<!DOCTYPE html>
<html>
  <head>
    <title>MailingListProject and Job Thing Example</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |key, value| %>
    <div class="flash <%= value %>">
      <%= value %>
    </div>
    <% end %>
    <%= yield %>
  </body>
</html>
